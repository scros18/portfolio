{
  "name": "sam-portfolio",
  "version": "1.0.0",
  "description": "High-performance TypeScript portfolio with glassy design",
  "main": "dist/main.js",
  "scripts": {
    "dev": "npm run build:dev && concurrently --names \"TS,SERVER\" --prefix-colors \"cyan,magenta\" \"npm run watch\" \"npm run serve\"",
    "build": "node build.js",
    "build:dev": "tsc",
    "build:prod": "node build.js",
    "watch": "tsc --watch --preserveWatchOutput",
    "serve": "node server.js",
    "start": "npm run dev",
    "deploy": "npm run build:prod && npm run verify",
    "verify": "node verify-build.js",
    "audit": "npm audit",
    "security": "npm audit --audit-level=moderate",
    "test": "echo \"No tests yet\" && exit 0"
  },
  "keywords": [
    "portfolio",
    "typescript",
    "glassmorphism",
    "performance"
  ],
  "author": "Sam",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cssnano": "^7.1.1",
    "html-minifier-terser": "^7.2.0",
    "javascript-obfuscator": "^4.1.1",
    "postcss": "^8.5.6",
    "postcss-cli": "^11.0.1",
    "terser": "^5.44.0",
    "typescript": "^5.3.3"
  }
}
